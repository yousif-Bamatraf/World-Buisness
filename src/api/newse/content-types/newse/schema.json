{
  "kind": "collectionType",
  "collectionName": "newses",
  "info": {
    "singularName": "newse",
    "pluralName": "newses",
    "displayName": "News"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "News_Title": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "required": true
    },
    "News_SubTitle": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "required": true
    },
    "News_Content": {
      "type": "richtext",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "required": true
    },
    "News_Date": {
      "type": "date",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "required": true
    },
    "News_images": {
      "type": "media",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "multiple": true,
      "required": false,
      "allowedTypes": ["images", "files"]
    },
    "New_Avatar": {
      "type": "media",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "multiple": false,
      "required": true,
      "allowedTypes": ["images", "files"]
    },
    "News_tags": {
      "type": "component",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "component": "shared.tags-list",
      "repeatable": true,
      "required": true
    }
  }
}
