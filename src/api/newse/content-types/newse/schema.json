{
  "kind": "collectionType",
  "collectionName": "newses",
  "info": {
    "singularName": "newse",
    "pluralName": "newses",
    "displayName": "News"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "News_Title": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "required": true
    },
    "News_SubTitle": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "required": true
    },
    "News_tags": {
      "type": "enumeration",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "required": false,
      "enum": [
        "Project Management",
        "Investment",
        "Feasibility Studies",
        "Institutional Development",
        "Entrepreneurship",
        "Startups",
        "Education and Training",
        "Digital Transformation",
        "Studies and Consulting"
      ]
    },
    "News_Content": {
      "type": "richtext",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "required": true
    },
    "News_Date": {
      "type": "date",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "required": true
    },
    "News_images": {
      "type": "media",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files"
      ]
    },
    "New_Avatar": {
      "type": "media",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images",
        "files"
      ]
    }
  }
}
