{
  "kind": "collectionType",
  "collectionName": "newses",
  "info": {
    "singularName": "newse",
    "pluralName": "newses",
    "displayName": "News"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "News_Title": {
      "type": "string",
      "required": true
    },
    "News_Sectors": {
      "type": "enumeration",
      "required": true,
      "enum": [
        "Finance",
        "Technical"
      ]
    },
    "News_Content": {
      "type": "richtext",
      "required": true
    },
    "News_Image": {
      "type": "media",
      "multiple": true,
      "required": true,
      "allowedTypes": [
        "images",
        "videos"
      ]
    },
    "News_Date": {
      "type": "date",
      "required": true
    },
    "News_Writer": {
      "type": "string",
      "required": true
    }
  }
}
