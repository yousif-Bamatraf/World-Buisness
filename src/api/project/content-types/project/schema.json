    "Project_Status": {
      "type": "enumeration",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "required": true,
      "enum": [
        "Active",
        "Planning",
        "Completed"
      ]
    },
    "Project_Image": {
      "type": "media",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "Sectors": {
      "type": "component",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "component": "shared.sectors",
      "repeatable": true,
      "required": true
