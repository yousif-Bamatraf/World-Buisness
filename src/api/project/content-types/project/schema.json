    "Project_Lead": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "required": true
    },
    "Project_Location": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "required": true
    },
    "Project_Description": {
      "type": "richtext",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "required": true
    },
    "Project_Start_Date": {
      "type": "date",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "required": true
    },
    "Project_End_Date": {
      "type": "date",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "required": false
